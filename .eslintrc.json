{
    "env": {
        "browser": true,
        "es2021": true,
        "jasmine": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-return-assign" :"off",
        "no-param-reassign" :"off",
        "no-plusplus" :"off",
        "no-console" :"off"
    }
}
